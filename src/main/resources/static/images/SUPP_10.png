<script>
    Handlebars.registerHelper("emphasisHeader", function(options) {
        return options.replace($("#searchProductHeader").val(),"<span>" + $("#searchProductHeader").val() + "</span>");
    });

    Handlebars.registerHelper("searchPath", function() {
        return getSearchPath();
    });

    var searchTarget = "SUPPLEMENT";

    $("#searchType").change(function(){
        // 변경된 값으로 비교 후 alert 표출
        if($(this).val() === "SUPPLEMENT"){
            searchTarget = "SUPPLEMENT";
            $("#searchProductHeader").attr('placeholder', '영양제 검색');
        }
        else if($(this).val() === "COLUMN") {
            searchTarget = "COLUMN";
            $("#searchProductHeader").attr('placeholder', '칼럼 검색');
        }
        else if($(this).val() === "WORKOUT") {
            searchTarget = "WORKOUT";
            $("#searchProductHeader").attr('placeholder', '운동 검색');
        }
        else {
            searchTarget = "FOOD";
            $("#searchProductHeader").attr('placeholder', '음식 검색');
        }
    });

    function getSearchPath() {
        if (searchTarget === 'FOOD') {
            return "/foods/search";
        } else if(searchTarget === 'COLUMN') {
            return "/columns";
        } else if(searchTarget === 'WORKOUT') {
            return "/workout/search";
        } else {
            return "/search";
        }
    }

    function changeSelect(option) {
        if (option === 'food') {
            location.href = '/foods';
        } else {
            location.href = '/search';
        }
    }

    function getKeywordPath() {
        if (searchTarget === "FOOD") {
            return "/web/api/foods/suggestKeywords";
        } else if (searchTarget === "WORKOUT") {
            return "/web/api/workout/suggestKeywords";
        } else if (searchTarget === "SUPPLEMENT") {
            return "/web/api/products/suggestKeywords";
        } else {
            return "";
        }
    }

    const RECENT_KEYWORD_KEY = "pillyze_recent_keyword";
    const RECENT_FOOD_KEYWORD_KEY = "pillyze_recent_food_keyword";
    const RECENT_FOOD_KEY = "pillyze_recent_food";
    const RECENT_WORKOUT_KEYWORD_KEY = "pillyze_recent_workout_keyword";
    let recentSearchKeywordList = new Array();
    let recentSearchFoodKeywordList = new Array();
    let recentSearchWorkoutKeywordList = new Array();
    let recentSearchFoodList = new Array();

    //최근 검색한 영양제 키워드를 저장하는 변수
    const savedKeywordList = JSON.parse(localStorage.getItem(RECENT_KEYWORD_KEY));
    if(savedKeywordList !== null) {
        recentSearchKeywordList = savedKeywordList
    }

    //최근 검색한 음식 키워드를 저장하는 변수
    const savedFoodKeywordList = JSON.parse(localStorage.getItem(RECENT_FOOD_KEYWORD_KEY));
    if(savedFoodKeywordList !== null) {
        recentSearchFoodKeywordList = savedFoodKeywordList
    }

    //음식상세 최근 조회한 음식 리스트를 저장하기 위한 변수
    const savedFoodList = JSON.parse(localStorage.getItem(RECENT_FOOD_KEY));
    if(savedFoodList !== null) {
        recentSearchFoodList = savedFoodList
    }

    const savedWorkoutKeywordList = JSON.parse(localStorage.getItem(RECENT_WORKOUT_KEYWORD_KEY));
    if (savedWorkoutKeywordList !== null) {
        recentSearchWorkoutKeywordList = savedWorkoutKeywordList
    }

    function saveFood(food) {
        if(recentSearchFoodList.filter((f) => f.foodIdEnc === food.foodIdEnc).length === 0){
            recentSearchFoodList.push(food);
            if (recentSearchFoodList.length > 4) {
                recentSearchFoodList.shift();
            }
            typeof(Storage) !== 'undefined' && localStorage.setItem(RECENT_FOOD_KEY, JSON.stringify(recentSearchFoodList));
        }
    }

    function saveKeyword() {
        if (recentSearchKeywordList.length > 5) {
            recentSearchKeywordList.shift();
        }
        typeof(Storage) !== 'undefined' && localStorage.setItem(RECENT_KEYWORD_KEY, JSON.stringify(recentSearchKeywordList));
    }

    function saveFoodKeyword() {
        if (recentSearchFoodKeywordList.length > 5) {
            recentSearchFoodKeywordList.shift();
        }
        typeof(Storage) !== 'undefined' && localStorage.setItem(RECENT_FOOD_KEYWORD_KEY, JSON.stringify(recentSearchFoodKeywordList));
    }

    function saveWorkoutKeyword() {
        if (recentSearchWorkoutKeywordList.length > 5) {
            recentSearchWorkoutKeywordList.shift();
        }
        typeof(Storage) !== 'undefined' && localStorage.setItem(RECENT_WORKOUT_KEYWORD_KEY, JSON.stringify(recentSearchWorkoutKeywordList));
    }

    function deleteRecentKeyword(k) {
        recentSearchKeywordList = recentSearchKeywordList.filter((keyword) => keyword !== k);
        saveKeyword();
    }

    function deleteRecentFoodKeyword(k) {
        recentSearchFoodKeywordList = recentSearchFoodKeywordList.filter((keyword) => keyword !== k);
        saveFoodKeyword();
    }

    function deleteRecentWorkoutKeyword(k) {
        recentSearchWorkoutKeywordList = recentSearchWorkoutKeywordList.filter((keyword) => keyword !== k);
        saveWorkoutKeyword();
    }

    function delay(callback, ms) {
        var timer = 0;
        return function() {
            var context = this, args = arguments;
            clearTimeout(timer);
            timer = setTimeout(function () {
                callback.apply(context, args);
            }, ms || 0);
        };
    }

    var suggestSourceHeader = $("#suggestKeywordListItemHeader").html();
    var suggestKeywordTemplateHeader = Handlebars.compile(suggestSourceHeader);

    $("#searchProductHeader").keypress(function(event) {
        if(event.keyCode===13){
            searchProductHeader();
        }
    });

    function searchProductHeader() {
        if($("#searchProductHeader").val().length > 0) {
            sessionStorage.setItem('prevDOM', '');
            sessionStorage.setItem('prevAnchor', '');
            recentSearchKeywordList = recentSearchKeywordList.filter((keyword) => keyword !== $("#searchProductHeader").val());
            recentSearchKeywordList.push($("#searchProductHeader").val());

            if (searchTarget === "FOOD") {
                saveFoodKeyword();
            } else if(searchTarget === "SUPPLEMENT"){
                saveKeyword();
            } else if(searchTarget === "WORKOUT"){
                saveWorkoutKeyword();
            }

            location.href = getSearchPath() + "?query=" + $("#searchProductHeader").val();
        }
    }

    $("#searchProductHeader").focus(function(event) {
        $("#searchProductHeader").css('text-align', 'left');
    });

    $(document).click(function(event) {
        if (!$(event.target).is("#searchProductHeader")) {
            $("#suggestDivHeader").removeClass('active')
        }
    });

    function restoreSearchResultList() {
        if (sessionStorage.getItem('prevDOM') !== null && sessionStorage.getItem('prevDOM').length > 0) {
            $('#itemList').html(sessionStorage.getItem('prevDOM'));
            scrollToTargetItem(sessionStorage.getItem('prevAnchor'));
            sessionStorage.setItem('prevDOM', '');
            sessionStorage.setItem('prevAnchor', '');
        }
    }

    $("#searchProductHeader").keyup(delay(function (e) {
        if (searchTarget !== 'COLUMN') {
            if ($("#searchProductHeader").val() !== '') {
                $("#suggestDivHeader").addClass('active')
                $.ajax({
                    url: getKeywordPath() + "?query=" + this.value,
                    method: "GET",
                    contentType: 'application/json',
                    dataType: "json"
                })
                    .done(function (response) {
                        if (response.success === true) {
                            if (response.result.length > 0) {
                                var html = suggestKeywordTemplateHeader(response);
                                $("#suggestKeywordHeader").empty();
                                $("#suggestKeywordHeader").append(html);

                                $('.header-suggest-keyword').click((e) => {
                                    e.preventDefault();
                                    $("#searchProductHeader").val($(e.currentTarget).data('keyword'));
                                    searchProductHeader();
                                });
                            } else {
                                $("#suggestDivHeader").removeClass('active')
                            }
                        }
                    });
            } else {
                $("#suggestDivHeader").removeClass('active')
            }
        }
    }, 500));

    $('.header-suggest-keyword').click((e) => {
        e.preventDefault();
        $("#searchProductHeader").val($(e.currentTarget).data('keyword'));
    });
</script>
<script>
