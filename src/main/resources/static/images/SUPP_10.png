    Kakao.init('d90b46c8da739fd3e868046433b111b7'); //발급받은 키 중 javascript키를 사용해준다.
    function kakaoLogin() {
        Kakao.Auth.login({
            success: function (response) {
                var kakaoToken = response.access_token;
                Kakao.API.request({
                    url: '/v2/user/me',
                    success: function (response) {
                        $.ajax({
                            url: "/api/auth/join/web",
                            method: "POST",
                            contentType: 'application/json',
                            dataType: "json",
                            data: JSON.stringify(
                                {
                                    socialLoginProvider: 'KAKAO',
                                    token: kakaoToken,
                                    kakaoUserName : response.kakao_account.name,
                                    kakaoUserEmail : response.kakao_account.email,
                                    kakaoUserPhone : response.kakao_account.phone_number,
                                    agreeMarketing: false,
                                    agreePrivacy: false,
                                    agreeTerm: false,
                                })
                        })
                        .done(function (response) {
                            if (response.result !== undefined && response.result.token !== undefined) {
                                $.cookie('_pauth', response.result.token, { expires: 1 ,path: '/' });
                                location.reload();
                            } else {
                                alert(response.errorMessage);
                            }
                        });
                    },
                    fail: function (error) {
                        console.log(error)
                    },
                })
            },
            fail: function (error) {
                console.log(error)
            },
        })
    }

    function kakaoLoginAndRedirect(redirectUrl) {
        Kakao.Auth.login({
            success: function (response) {
                var kakaoToken = response.access_token;
                Kakao.API.request({
                    url: '/v2/user/me',
                    success: function (response) {
                        $.ajax({
                            url: "/api/auth/join/web",
                            method: "POST",
                            contentType: 'application/json',
                            dataType: "json",
                            data: JSON.stringify(
                                {
                                    socialLoginProvider: 'KAKAO',
                                    token: kakaoToken,
                                    kakaoUserName : response.kakao_account.name,
                                    kakaoUserEmail : response.kakao_account.email,
                                    kakaoUserPhone : response.kakao_account.phone_number,
                                    agreeMarketing: false,
                                    agreePrivacy: false,
                                    agreeTerm: false,
                                })
                        })
                        .done(function (response) {
                            if (response.result !== undefined && response.result.token !== undefined) {
