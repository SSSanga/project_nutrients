<script src="https://pagination.js.org/dist/2.1.5/pagination.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/2.0.10/clipboard.min.js"></script>
<script id="reviewMoreList" type="text/x-handlebars-template">
	{{#each data}}
	<li class="review-content blur">
			<button tabindex="0" class="btn-goto-login" onclick="kakaoLogin()"><span>3초 만에 로그인하고 리뷰 보기</span></button>
			<button tabindex="0" class="btn-goto-app" onclick="goToDownload()"><span>앱으로 리뷰 보기</span></button>
		<span class="user-name">{{this.userName}}</span>
		<span class="user-agesex">{{this.ageBand}} / {{this.gender}}</span>
		<div class="review-info-row1">
			<div class="star-point-small-bg">
				<div class="star-point-small" style="width: {{score this.score}}px"></div>
			</div>
			<span class="date">{{{dateFormat this.creationTime}}}</span>
		</div>
		<div class="review-info-row2">
			<div class="txt1">
				{{period this.intakePeriod}}
			</div>
			{{{certificate this.certificated}}}
		</div>
		{{{formeTester this.reviewerType}}}
		{{{writeText this.goodText this.badText}}}
		{{{writeTag '효과' this.goodEffectList 'card-tag2' this.status}}}
		{{{writeTag '부작용' this.badEffectList 'card-tag1' this.status}}}
        {{#if (hasImageList this.imageList this.status)}}
        <div class="product-pic-row">
            <div class="pic-row-inside">
                {{#each this.imageList}}
                <a tabindex="0" class="img1wrap"><img src="{{this.thumbnailUrl}}" class="img1" alt="사용자 첨부 사진"></a>
                {{/each}}
            </div>
        </div>
        {{/if}}
	</li>
	{{/each}}
</script>
<script id="questionList" type="text/x-handlebars-template">
	{{#each data}}
	<div class="qna-card">
		<a tabindex="0" class="qna-main" title="이 Q&A의 상세정보 보기" href="/qna/detail/{{this.id}}">
			<span class="txt1"><span class="qmark">Q.&nbsp;</span>{{this.title}}</span>
			<span class="txt2">{{this.questionContent}}</span>
		</a>
	</div>
	{{/each}}
</script>
<script>
    setRecommendProductSwiper();
    $(window).resize(function () {
        setRecommendProductSwiper();
    });

    //현재 선택된 영양성분 필터
    var currentCriteria;

    Handlebars.registerHelper('writeText', function (good, bad) {
        if (good !== undefined && good !== '텍스트 리뷰는 작성하지 않았어요') {

            var goodElement = '<span class="sub-title-small">Good</span><p class="review-full-txt">' + good.replaceAll("\n", "<br>") + '</p>';
            var badElement = '<span class="sub-title-small sts2">Bad</span><p class="review-full-txt">' + bad.replaceAll("\n", "<br>") + '</p>';

            return goodElement + badElement;
        }
    });

    Handlebars.registerHelper('writeTag', function (title, tags, colorClass, status) {
        if (tags !== undefined && tags.length > 0 && status === 'NORMAL') {
            var tagElement = '<div class="sub-title-wrap"><span class="sub-title">' + title + '</span></div><div class="card-tags">';
            for (let i = 0; i < tags.length; i++) {
                tagElement = tagElement + '<div class="card-tag ' + colorClass + '">' + tags[i] + '</div>';
            }
            return tagElement + '</div>';
        }
    });

    Handlebars.registerHelper('hasImageList', function (imageList, status) {
        return imageList !== undefined && imageList.length > 0 && status === 'NORMAL';
    });

    Handlebars.registerHelper('replaceBR', function (options) {
        return options.replaceAll("\n", "<br>");
    });

    Handlebars.registerHelper("certificate", function (options) {
        if (options === true) {
            return '<div class="txt2">이 제품의 섭취 기록이 있는 유저에요</div>';
        }
    });

    Handlebars.registerHelper("formeTester", function (options) {
        if (options === 'SUPPORTER_FOR_ME_TESTER') {
            return '<div class="p-review-tester1">테스터 리뷰</div><div class="p-review-tester2">제품을 무료로 제공받아 직접 섭취 후 솔직하게 작성한 리뷰에요.</div>';
        }
    });

    Handlebars.registerHelper("period", function (options) {
        if (options === 'OVER_A_YEAR') return '섭취: 1년 이상';
        if (options === 'OVER_SIX_MONTH') return '섭취: 6개월 이상';
        if (options === 'OVER_THREE_MONTH') return '섭취: 3개월 이상';
        if (options === 'OVER_A_MONTH') return '섭취: 1개월 이상';
        return '섭취: 1개월 이하'
    });

    Handlebars.registerHelper("score", function (options) {
        if (options === 5) return 82;
        if (options === 4) return 68;
        if (options === 3) return 51;
        if (options === 2) return 34;
        return 17;
    });

    Handlebars.registerHelper("dateFormat", function (options) {
        return moment(options).format('YYYY/MM/DD')

    });

    function checkOverflow(el) {
        var curOverf = el.style.overflow;

        if (!curOverf || curOverf === "visible")
            el.style.overflow = "hidden";

        var isOverflowing = el.clientWidth < el.scrollWidth
            || el.clientHeight < el.scrollHeight;

        el.style.overflow = curOverf;

        return isOverflowing;
    }

    function checkTagOverflow() {
        if (document.getElementById('tagFilters') !== null) {
            var isOverflow = checkOverflow(document.getElementById('tagFilters'));
            if (!isOverflow) {
                $('#showTagAll').hide();
            }
        }
    }

    checkTagOverflow();

    function showTagAll() {
        if ($('#showTagAll').text() === '모두 보기') {
            $('#tagFilters').css('height', 'auto');
            $('#showTagAll').text('접기')
        } else {
            $('#tagFilters').css('height', '100px');
            $('#showTagAll').text('모두 보기')
        }

    }

    function showNutrientAll() {
        $('.nutrient-more-card').css('display', 'block');
        $('#nutrientSummaryText').addClass('active');
        $('#showMoreNutrient').hide();
        $('#hideMoreNutrient').show();
    }

    function hideNutrientAll() {
        $('.nutrient-more-card').css('display', 'none');
        $('#nutrientSummaryText').removeClass('active');
        $('#showMoreNutrient').show();
        $('#hideMoreNutrient').hide();
        // document.getElementById('nutrientCardDiv').scrollIntoView();
    }

    function filterNutrient(criteria) {
        if (currentCriteria === criteria) {
            showAllNutrient();
        } else {
            $('.nutrient-card').hide();
            $('.' + criteria).show();
            $('.filter-nutrient-btn').removeClass('active');
            $('.filter-nutrient-btn.' + criteria).addClass('active');
            $('#showMoreNutrient').hide();
            $('#hideMoreNutrient').hide();
            currentCriteria = criteria;
        }

    }

    function showAllNutrient() {
        $('.nutrient-card').show();
        $('.filter-nutrient-btn').removeClass('active');
        $('.filter-nutrient-btn.all').addClass('active');
        $('#showMoreNutrient').hide();
        $('#hideMoreNutrient').show();
        currentCriteria = '';
    }

    var sort = 'BEST';
    var tag = '';
    var page = 1;
    var reviewMoreSource = $("#reviewMoreList").html();
    var reviewMoreTemplate = Handlebars.compile(reviewMoreSource);
    var boarding = true;

    function goToDownload() {
        location.href = globalDownloadLink;
    }

    function buildPagination() {
        $('#pagination-container').pagination({
            dataSource: '/web/api/products/2436/reviews?sort=' + sort + '&tag=' + tag,
            pageNumber: page,
            showFirstOnEllipsisShow: false,
            showLastOnEllipsisShow: false,
            pageSize: 10,
            pageRange: 2,
            prevText: '',
            nextText: '',
            locator: 'result.content',
            totalNumberLocator: function (response) {
                // you can return totalNumber by analyzing response content
                return response.result.totalElements;
            },
            alias: {
                pageNumber: 'page',
            },
            ajax: {
                beforeSend: function () {
                    // $('#data-container').html('Loading data from flickr.com ...');
                }
            },
            callback: function (data, pagination) {
                $('.p-go-to-download').click(function () {
                    location.href = globalDownloadLink;
                });
                var totalPageCount = Math.ceil(pagination.totalNumber / 5);

                if (totalPageCount === 1) {
                    $('.paginationjs').hide();
                }

                var lastIndex = $('.paginationjs-page.J-paginationjs-page').length - 1;

                if (totalPageCount > 5) {
                    if (pagination.pageNumber === 4) {
                        $($('.paginationjs-page.J-paginationjs-page')[0]).hide();
                    } else if (pagination.pageNumber === 5) {
                        $($('.paginationjs-page.J-paginationjs-page')[0]).hide();
                        $($('.paginationjs-page.J-paginationjs-page')[1]).hide();
                    }
                }

                if (totalPageCount > 7) {
                    if (totalPageCount - pagination.pageNumber === 4) {
                        $($('.paginationjs-page.J-paginationjs-page')[lastIndex]).hide();
                        $($('.paginationjs-page.J-paginationjs-page')[lastIndex - 1]).hide();
                    } else if (totalPageCount - pagination.pageNumber === 3) {
                        $($('.paginationjs-page.J-paginationjs-page')[lastIndex]).hide();
                    }
                }

                if (totalPageCount === 6) {
                    if (pagination.pageNumber < 4) {
                        $($('.paginationjs-page.J-paginationjs-page')[lastIndex]).hide();
                    } else if (pagination.pageNumber === 6) {
                        $($('.paginationjs-page.J-paginationjs-page')[0]).hide();
                    }
                }

                if (totalPageCount === 7) {
                    if (pagination.pageNumber < 4) {
                        $($('.paginationjs-page.J-paginationjs-page')[lastIndex]).hide();
                        $($('.paginationjs-page.J-paginationjs-page')[lastIndex - 1]).hide();
                    } else if (pagination.pageNumber === 6) {
                        $($('.paginationjs-page.J-paginationjs-page')[0]).hide();
                        $($('.paginationjs-page.J-paginationjs-page')[1]).hide();
                    } else if (pagination.pageNumber === 7) {
                        $($('.paginationjs-page.J-paginationjs-page')[0]).hide();
                        $($('.paginationjs-page.J-paginationjs-page')[1]).hide();
                    }
                }

                var html = reviewMoreTemplate({
                    data: data
                });
                $('#data-container').html(html);
                if (pagination.pageNumber === 1) {
                    for (let i = 0; i < 5; i++) {
                        $($('.review-content.blur')[0]).removeClass("blur");
                    }
                }
                //활성화시 페이지 변경이 감지되면 리뷰 상단으로 이동해줌
                if (!boarding) {
                    scrollToTargetAdjusted();
                } else {
                    boarding = false;
                }
            }
        })
    }


    function changeTag(option, index) {
        page = 1;
        if (tag === option) {
            tag = '';
            $('.review-tag-btn').removeClass('active');
        } else {
            tag = option;
            $('.review-tag-btn').removeClass('active');
            $('#tag' + index).addClass('active');
        }
        buildPagination();
    }

    function changeSort(option) {
        sort = option;
        page = 1;
        $('.bs-option').removeClass('active');
        $('#' + option).addClass('active');
        buildPagination();
    }

    function goToReview() {
        var element = document.getElementById('startReview');
        var headerOffset = 120;
        var elementPosition = element.getBoundingClientRect().top;
        var offsetPosition = elementPosition + window.scrollY - headerOffset;

        window.scrollTo({
            top: offsetPosition,
            behavior: "smooth"
        });
    }

    $('.search-brand').click((e) => {
        e.preventDefault();
        sessionStorage.setItem('prevDOM', '');
        sessionStorage.setItem('prevAnchor', '');
        location.href = $(e.currentTarget).attr('href');
    })

    $('.top-nav').click((e) => {
        e.preventDefault();
        sessionStorage.setItem('prevDOM', '');
        sessionStorage.setItem('prevAnchor', '');
        location.href = $(e.currentTarget).attr('href');
    })

    function scrollToTargetAdjusted() {
        var element = document.getElementById('data-container');
        var headerOffset = 120;
        var elementPosition = element.getBoundingClientRect().top;
        var offsetPosition = elementPosition + window.scrollY - headerOffset;

        window.scrollTo({
            top: offsetPosition,
            behavior: "smooth"
        });
    }

    var clipboard = new ClipboardJS('.btn-share');
    clipboard.on('success', function (e) {
        alert("주소가 복사되었습니다.");
    });

    var swiper = new Swiper('.swiper-container2', {
        slidesPerView: 3,
        centeredSlides: false,
        spaceBetween: 20,
        grabCursor: true,
        loop: false,
        pagination: {
            el: '.swiper-pagination-column',
            clickable: true,
        },
        navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
        },
        /*autoplay:{
            delay:2500,
        },*/
    });

    var swiper = new Swiper('.swiper-container3', {
        slidesPerView: 2,
        centeredSlides: false,
        spaceBetween: 20,
        grabCursor: true,
        loop: false,
        pagination: {
            el: '.swiper-pagination-column',
            clickable: true,
        },
        navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
        },
        /*autoplay:{
            delay:2500,
        },*/
    });

    var swiper = new Swiper('.swiper-container4', {
        slidesPerView: 1,
        centeredSlides: false,
        spaceBetween: 20,
        grabCursor: true,
        loop: false,
        pagination: {
            el: '.swiper-pagination-column',
            clickable: true,
        },
        /*autoplay:{
            delay:2500,
        },*/
    });

    $(".user-review-wrap-1 .title-wrap").click(function () {
        if ($('.user-review-wrap-1').hasClass('active')) {
            $('.user-review-wrap-1').removeClass('active');
            $('.review-summary').slideUp("slow");
        } else {
            $('.user-review-wrap-1').addClass('active');
            $('.review-summary').slideDown("slow");
        }
    });

    /*제품상세 : 1일 섭취량 기준 함량 정보 - 모두보기 버튼 클릭*/
    $('.imw-show').click(function () {
        $(this).removeClass('active');
        $(this).next().addClass('active');
        $(this).next().next().addClass('active');
    });


    /*제품상세 : 1일 섭취량 기준 함량 정보 - 모두보기 버튼 클릭*/
    $('.imw-hide').click(function () {
        $(this).removeClass('active');
        $(this).prev().removeClass('active');
        $(this).prev().prev().addClass('active');
    });


    function showProductDetail() {
        $('#tab-product-detail').addClass('active');
        $('#tab-review').removeClass('active');
        $('#tab-introduce').removeClass('active');

        $('#product-detail').show()
        $('#review').show()
        $('#introduce').show()
        goToElement('tab-dummy')
    }

    function showReview() {
        $('#tab-product-detail').removeClass('active');
        $('#tab-review').addClass('active');
        $('#tab-introduce').removeClass('active');

        $('#product-detail').hide()
        $('#review').show()
        $('#introduce').hide()
        goToElement('tab-dummy')
    }

    function showIntroduce() {
        $('#tab-product-detail').removeClass('active');
        $('#tab-review').removeClass('active');
        $('#tab-introduce').addClass('active');

        $('#product-detail').hide()
        $('#review').hide()
        $('#introduce').show()

        goToElement('tab-dummy')
    }

    function goToElement(id) {
        var element = document.getElementById(id);
        var headerOffset = $('#nav-header').height();
        var elementPosition = element.getBoundingClientRect().top;
        var offsetPosition = elementPosition + window.scrollY - headerOffset;

        window.scrollTo({
            top: offsetPosition,
            behavior: "smooth"
        });
    }

    function toggleIntroduce() {
        if ($('#btn-introduce-more').hasClass('active')) {
            $('#btn-introduce-more').removeClass('active');
            $('#btn-introduce-more').text('상품 정보 더보기');
            $('#introduce-content').removeClass('active');
            $('#productInfoGradient').addClass('active');

            var element = document.getElementById('introduce');
            var headerOffset = $('#nav-header').height();
            var elementPosition = element.getBoundingClientRect().top;
            var offsetPosition = elementPosition + window.scrollY - headerOffset;

            window.scrollTo({
                top: offsetPosition,
                behavior: "smooth"
            });
        } else {
            $('#btn-introduce-more').addClass('active');
            $('#btn-introduce-more').text('상품 정보 접기');
            $('#introduce-content').addClass('active');
            $('#productInfoGradient').removeClass('active');
        }
    }


    $(window).scroll(function () {
        var tabTop = $('#tab-dummy').offset().top;
        var scroll = $(window).scrollTop();
        var navTop = $('#nav-header').height() + scroll - $('#tab-dummy').height();


        if (navTop > tabTop && !$('#product-detail-tab').hasClass('sticky')) {
            $('#product-detail-tab').addClass('sticky');
            $('#nav-header').addClass('sticky');
            $('#product-detail-tab').css('width', $('.new-wide-main').width());
        } else if (navTop < tabTop && $('#product-detail-tab').hasClass('sticky')) {
            $('#product-detail-tab').removeClass('sticky');
            $('#nav-header').removeClass('sticky');
            $('#product-detail-tab').css('width', '100%');
        }
    });

    $(window).resize(function () {
        $('#product-detail-tab').css('width', $('.new-wide-main').width());
    });


    function goToBuy() {
        $.ajax({
            url: "/web/api/2436/affiliateShop",
            method: "GET",
            contentType: 'application/json',
            dataType: "json"
        })
            .done(function (response) {
                if (response.success === true) {

                    if (response.result !== undefined && response.result.data !== undefined && response.result.data.landingUrl !== undefined) {
                        if (typeof (window.open) == "function") {
                            window.open(response.result.data.landingUrl, '_blank');
                        }
                        else {
                            location.href = response.result.data.landingUrl;
                        }
                    } else {
                        alert('일시적으로 구매정보를 불러올 수 없습니다.');
                    }
                }
            });
    }


    function guidePopupOpen() {
        $('#guidePopup').addClass('active');
        $('#guidePopupClose').addClass('active');
        $('html').addClass('body-no-scroll');
    }
</script>
</body>
</html>
